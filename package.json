{
  "name": "fictioneers-node-sdk",
  "version": "1.2.3",
  "description": "Server-side javascript SDK wrapping the Fictioneers API",
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "ts3.1/*"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-api-types": "openapi-typescript https://api.fictioneers.co.uk/_docs/openapi --output ./src/openapi.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fictioneers/node-sdk.git"
  },
  "keywords": [],
  "author": "Fictioneers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fictioneers/node-sdk/issues"
  },
  "homepage": "https://github.com/fictioneers/node-sdk#readme",
  "dependencies": {
    "node-fetch": "^3.2.1",
    "uuid": "^8.3.2"
  },
  "type": "module",
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "typescript": "^4.9.3",
    "openapi-typescript": "^5.4.1"
  },
  "exports": {
    ".": {
      "types": {
        "default": "./src/index.d.ts",
        "import": "./src/index.d.ts"
      }
    }
  }
}
